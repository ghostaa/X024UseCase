	<context id="saveMoneyOpCtx" type="op">
		<refKColl refId="saveMoneyOpRecord"/>
	</context>

	<kColl id="saveMoneyOpRecord">
		<data id="username" refType="String"/>
		<data id="password" refType="String"/>
		<data id="userAccount" refType="Number"/>
		<data id="saveMoney" refType="Number"/>
		<data id="withdrawMoney" refType="Number"/>
		<iColl id="userLevel">
			<kColl id="userLevel">
				<data id="commonUser" refType="String"/>
				<data id="vipUser" refType="String"/>
			</kColl>
		</iColl>
		<data id="message" refType="String"/>
	</kColl>

</saveMoneyOpOp>
